# Google AI 服务配置
# 必需 - 从 Google AI Studio 获取 (https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=your_google_gemini_api_key_here

# 可选 - Google Cloud 项目配置
GOOGLE_PROJECT_ID=your_google_cloud_project_id
VERTEX_AI_LOCATION=us-central1

# 应用配置
NODE_ENV=development
PORT=3000
API_TIMEOUT=30000

# Gemma 本地模型配置
GEMMA_MODEL_PATH=./models/gemma-7b-it
GEMMA_DEVICE_TYPE=cpu
GEMMA_MAX_TOKENS=512
GEMMA_TEMPERATURE=0.7

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json

# 缓存配置
ENABLE_CACHE=true
CACHE_TTL=3600

# 安全配置
ENABLE_RATE_LIMIT=true
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 